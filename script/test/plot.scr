main()
{
	plotparams = map();
	plotparams["xlabel"] = "x";
	plotparams["ylabel"] = "y";
	plotparams["ylimits"] = "-1.1 1.1";

	i=0;
	while(i less 100)
	{
		plotparams["title"] = "Parameter: " + i;

		ph = i/500.;
		x = linspace(0, 2*pi, 64);
		y = sin(x + ph);

#		print(plotparams);
		plot(x,y, plotparams);

		i = i+1;
	}
}
