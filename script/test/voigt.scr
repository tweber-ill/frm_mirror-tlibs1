voigt(x, sigma, gamma)
{
	z = complex(x, gamma) / (sigma * sqrt(2.));
	return real(faddeeva(z)) / (sigma * sqrt(2.*pi));
}


main()
{
#	print(erf_cplx(1.23));
#	print(faddeeva(1.23));
#	print(voigt(0., 1., 1.));

	x = linspace(-10., 10., 128);
	y = [];
	for(thex : x)
	{
		they = voigt(thex, 1., 1.);
		y[length(y)] = they;
		
	}

	plot(x,y, ["style":"line"]);
}
