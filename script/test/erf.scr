main()
{
	x = linspace(-3, 3, 128);
	y = erf(x)^2;
	y2 = erf(2.*x)^2;
	y3 = tanh(x)^2;

	opt = ["style":"line", "legend":"erf^2(x)"];
	opt2 = ["style":"line", "legend":"erf^2(2x)"];
	opt3 = ["style":"line", "legend":"tanh^2(x)"];

	plot([[x,y,opt], [x,y2,opt2], [x,y3, opt3]]);
}


S(x)
{
	return 1./(1. + exp(-x));
}
